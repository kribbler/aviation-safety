.foto-pattern{
	background: none;
}

#dashboard_items{
	list-style: none;
	margin: 30px 0;
}
#dashboard_items li{
	display: block;
	margin: 0 30px 20px 0;
	
}

#dashboard_items li a, #wppb-submit, .user_info_submit{
	-webkit-border-radius: 10px;
	-moz-border-radius: 10px;
	border-radius: 10px;
	background-color: #363636 !important;
	color: #fff;
	padding: 15px 30px;
	display: inline-block;
	border: 0;
	box-shadow: none !important;
}

