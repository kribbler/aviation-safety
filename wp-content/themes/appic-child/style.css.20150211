@import url('../appic/style.css');
@import url(http://fonts.googleapis.com/css?family=Lato:400,700);

body{
	font-family: 'Lato', sans-serif;
	background-color: #05519c;
}

#body_questions{
	background-color: rgb(209,225,243);
}
.hidden{
	display: none;
}
h1, h2, h3, h4, h5, h6, p{
	font-family: 'Lato', sans-serif;
}

a{
	color: #10b0e7;
}

h4{
	font-size: 30px;
	font-weight: bold;
	margin-bottom: 20px;
	line-height: 36px;
}


h5{
	font-size: 30px;
	font-weight: normal;
	margin-bottom: 20px;
	line-height: 36px;
}

.logo{
	margin-bottom: 30px;
    margin-top: 36px;
    padding: 0;
}
.clear{
	clear: both;
}
.main-nav-wrap.fixed-pos{
	position: inherit;
}

.blue-line-wrap:before{
	background: none !important;
}

.blue-line-wrap{
	border-top: 0;
	background: none;
	margin-top: 0;
}

.main-nav-wrap{
	background: #fff;
	border-top: 0;
}

.white-wrap{
	background: none;
}

.action-area-mini{
	background: #05519c;
	padding: 0;
	color: #fff;
	margin-top: -5px;
}

.action-area-mini .span2{
	font-size: 13px;
	color: #f9fb00;
	text-align: right;
}

.heading{
	font-size: 25px;
	padding: 15px 0;
}

#navigation-box{
	/*display: none;*/
	position: relative;
	background: #157ebf;
	display: inline-block;
	float: none;
	width: 100%;
	margin-bottom: -5px;
}


ul#navigation, .menu ul{
	float: none;
	margin-right: auto;
	height: auto;
}
ul#navigation a, .menu ul a{
	color: #575859;
	font-weight: normal !important;
    letter-spacing: 1px !important;
    text-transform: uppercase;
}

ul#navigation > li, .menu ul > li{
	min-height: 1px;
	padding: 0px 20px;
}
ul#navigation > li > a{
	font-family: "Geometria-Medium", sans-serif;
}
ul#navigation > li > a, .menu ul > li > a{
	padding: 5px 5px 5px 5px;
        font-weight: normal;
    letter-spacing: 1.1px;
    border-bottom: 0px solid #fff;
    margin-right: 15px;
    color: #fff;
}

ul#navigation > li > a:hover, .menu ul > li > a:hover{
	background: none !important;
	color: #fff;
}


ul#navigation > li:hover{
	border-bottom: 0 !important;
	padding-bottom: 0 !important;
	background: none;
}

ul#navigation a:hover,
ul#navigation .active-item > a,
ul#navigation .current-menu-item > a,
ul#navigation .current-menu-parent > a {
	color: #fff;
	/*background: #fff !important;*/
	border-bottom: 2px solid #4aa8dd;
}

.main-navbar{
	min-height: 1px;
}

.home-bg{
	background: #fff url(images/bg_home-page.jpg) center top no-repeat;
	background-size: 100%;
	padding-top: 470px;
}

.footer-widget-wrap{
	background: none;
}

.footer-widget{
	background: url(images/bg_footer.jpg) center bottom no-repeat;
	background-size: 100%;
	padding-top: 0;
	min-height: 50px;
}

#home-page-inside{
	margin: 0 50px;
}

#logo_right{
	float: right;
	margin-top: 10px;
	text-align: right;
	position: relative;
	z-index: 1000;
}

#avatar_holder{
	bottom: 0;
    display: inline-block;
    position: absolute;
    right: 220px;
    top: 0px;
    width: 200px;
}
.avatar_hidden{
	position: absolute;
	top: -500px;
}
.whiteish{
	background-color: #bcdaf4;
}

#customer_login{
	width: 75%;
	background: #cde5fd;
	-webkit-border-radius: 25px;
	-moz-border-radius: 25px;
	border-radius: 25px;
	margin: 0 auto;
	padding: 30px;
}

.login-remember{
	display: none;
}

#loginform .input{
	height: 40px;
	padding: 3px 10px;
	-webkit-border-radius: 10px;
	-moz-border-radius: 10px;
	border-radius: 10px;
}

.wppb-form-field label, #wppb-login-wrap .login-username label, #wppb-login-wrap .login-password label{
	line-height: 40px;
	font-size: 20px;
}
